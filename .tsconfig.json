{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "esNext",
    "target": "es5",
    "jsx": "react",
    "allowJs": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "typeRoots": ["./src/@typings"],
    "baseUrl": ".",
    "moduleResolution": "node",
    "downlevelIteration": true,
    "lib": ["dom", "es7", "dom.iterable"]
  }
}
