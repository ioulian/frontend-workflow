{
  "name": "frontend-workflow",
  "version": "0.6.1",
  "description": "Frontend workflow using Webpack 4",
  "author": "Yulian Alexeyev <yulian@dev-q.be> (https://github.com/ioulian)",
  "keywords": [
    "css",
    "js",
    "webpack",
    "typescript",
    "post-css",
    "prettier",
    "nvm",
    "stylelint",
    "tslint",
    "editorconfig",
    "favicons",
    "bulma",
    "es6",
    "drupal"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ioulian/frontend-workflow",
  "repository": {
    "type": "git",
    "url": "git@github.com:ioulian/frontend-workflow.git"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "private": false,
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "dependencies": {
    "@types/throttle-debounce": "1.1.0",
    "bulma": "0.7.2",
    "onfire.js": "2.0.0",
    "throttle-debounce": "2.1.0"
  },
  "scripts": {
    "start": "npx webpack-dev-server --open --config webpack.dev.js",
    "watch": "npx webpack --watch --config webpack.dev.js",
    "build": "yarn lint && yarn export",
    "export": "NODE_ENV=production npx webpack --config webpack.prod.js",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "npx typescript --noEmit --project .tsconfig.json && npx tslint 'src/**/*.ts' -t verbose -c .tslint.json",
    "lint:css": "npx stylelint 'src/**/*.scss'"
  },
  "devDependencies": {
    "@csstools/postcss-sass": "4.0.0",
    "app-manifest-webpack-plugin": "1.2.0",
    "autoprefixer": "9.4.5",
    "clean-webpack-plugin": "1.0.0",
    "css-loader": "2.1.0",
    "cssnano": "4.1.8",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "imagemin-webpack-plugin": "2.4.2",
    "inline-manifest-webpack-plugin": "4.0.2",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "4.11.0",
    "postcss-css-variables": "0.11.0",
    "postcss-easing-gradients": "3.0.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "postcss-utilities": "0.8.0",
    "prettier": "1.16.0",
    "prettier-webpack-plugin": "1.2.0",
    "sass-loader": "7.1.0",
    "script-ext-html-webpack-plugin": "2.1.3",
    "script-loader": "0.7.2",
    "style-loader": "0.23.1",
    "stylelint": "9.10.1",
    "stylelint-config-standard": "18.2.0",
    "stylelint-webpack-plugin": "0.10.5",
    "sw-precache-webpack-plugin": "0.11.5",
    "ts-loader": "5.3.3",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.17.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-webpack-plugin": "2.0.2",
    "typescript": "3.2.2",
    "webpack": "4.28.4",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.8",
    "webpack-merge": "4.1.4",
    "webpack-plugin-critical": "1.0.0",
    "webpack-stream": "5.1.1"
  },
  "config": {
    "publicPath": "https://frontend-workflow.firstsite.be",
    "cacheId": "frontend-workflow",
    "name": "Frontend workflow",
    "shortName": "Frontend",
    "description": "Frontend workflow description",
    "author": "Yulian Alexeyev",
    "authorUrl": "https://github.com/ioulian",
    "theme": "#00d1b2",
    "background": "#fff",
    "modules": {
      "favicons": true,
      "criticalCSS": true,
      "swPrecache": true,
      "asyncJS": true
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "ie >= 10",
    "not op_mini all"
  ]
}
