{
  "name": "frontend-workflow",
  "version": "0.27.1",
  "description": "Frontend workflow using Webpack 4",
  "author": "Yulian Alexeyev <yulian@dev-q.be> (https://github.com/ioulian)",
  "keywords": [
    "css",
    "js",
    "webpack",
    "typescript",
    "post-css",
    "prettier",
    "nvm",
    "stylelint",
    "eslint",
    "editorconfig",
    "favicons",
    "bootstrap",
    "es6",
    "drupal",
    "drupal8",
    "web"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ioulian/frontend-workflow",
  "repository": {
    "type": "git",
    "url": "git@github.com:ioulian/frontend-workflow.git"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "private": false,
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "dependencies": {
    "@fancyapps/fancybox": "3.5.7",
    "bootstrap": "4.4.1",
    "core-js": "3.4.2",
    "cosmiconfig": "6.0.0",
    "element-closest": "3.0.2",
    "element-matches-polyfill": "1.0.0",
    "intersection-observer": "0.7.0",
    "jquery": "3.4.1",
    "onfire.js": "2.0.1",
    "promise-polyfill": "8.1.3",
    "smoothscroll-polyfill": "0.4.4",
    "swiper": "5.2.1",
    "throttle-debounce": "2.1.0",
    "vanilla-lazyload": "12.4.0",
    "workbox-webpack-plugin": "4.3.1"
  },
  "scripts": {
    "start": "NODE_ENV=devserver webpack-dev-server --open --config webpack.dev.js",
    "start:d": "NODE_ENV=devserver webpack-dashboard -- webpack-dev-server --open --config webpack.dev.js",
    "watch": "webpack --watch --config webpack.dev.js",
    "build": "yarn lint && yarn export",
    "export": "NODE_ENV=production webpack --config webpack.prod.js",
    "lint": "yarn tslint:lint && yarn eslint:lint && yarn stylelint:lint",
    "tslint:lint": "tsc --noEmit --project .tsconfig.json",
    "eslint:lint": "eslint 'src/**/*.{js,ts}'",
    "eslint:fix": "eslint --fix 'src/**/*.{js,ts}'",
    "stylelint:lint": "stylelint 'src/**/*.{scss,css}'",
    "stylelint:fix": "stylelint --fix 'src/**/*.{scss,css}'",
    "prettier:lint": "prettier --check 'src/**/*.{js,scss,css,ts}'",
    "prettier:fix": "prettier --write 'src/**/*.{js,scss,css,ts}'",
    "analyze": "NODE_ENV=production webpack --config webpack.prod.js --profile --json > stats.json && webpack-bundle-analyzer stats.json --default-sizes gzip"
  },
  "devDependencies": {
    "@csstools/postcss-sass": "4.0.0",
    "@intracto/eslint-config-intracto": "0.1.9",
    "@intracto/stylelint-config-intracto": "0.1.11",
    "@types/swiper": "5.2.0",
    "@types/throttle-debounce": "2.1.0",
    "@typescript-eslint/eslint-plugin": "2.7.0",
    "@typescript-eslint/parser": "2.7.0",
    "autoprefixer": "9.7.2",
    "clean-webpack-plugin": "3.0.0",
    "critters-webpack-plugin": "2.5.0",
    "css-loader": "3.2.0",
    "cssnano": "4.1.10",
    "eslint": "6.1.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "favicons-webpack-plugin": "2.1.0",
    "file-loader": "5.0.2",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "6.0.0",
    "mini-css-extract-plugin": "0.8.0",
    "node-sass": "4.13.0",
    "postcss-css-variables": "0.14.0",
    "postcss-easing-gradients": "3.0.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-utilities": "0.8.4",
    "prettier": "1.19.1",
    "prettier-webpack-plugin": "1.2.0",
    "sass-loader": "8.0.0",
    "script-ext-html-webpack-plugin": "2.1.4",
    "script-loader": "0.7.2",
    "style-loader": "1.0.0",
    "stylelint": "11.1.1",
    "stylelint-config-prettier": "6.0.0",
    "stylelint-config-recommended-scss": "4.1.0",
    "stylelint-prettier": "1.1.1",
    "stylelint-scss": "3.13.0",
    "svg-sprite-loader": "4.2.0",
    "svgo": "1.3.2",
    "svgo-loader": "2.2.1",
    "ts-loader": "6.2.1",
    "typescript": "3.7.2",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dashboard": "3.2.0",
    "webpack-dev-server": "3.9.0",
    "webpack-merge": "4.2.2",
    "webpack-stream": "5.2.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "ie >= 10",
    "not op_mini all"
  ]
}
