{
  "name": "frontend-workflow",
  "version": "0.31.1",
  "description": "Frontend workflow using Webpack 4",
  "author": "Yulian Alexeyev <yulian@dev-q.be> (https://github.com/ioulian)",
  "keywords": [
    "css",
    "js",
    "webpack",
    "typescript",
    "post-css",
    "prettier",
    "nvm",
    "stylelint",
    "eslint",
    "editorconfig",
    "favicons",
    "bootstrap",
    "es6",
    "drupal",
    "drupal8",
    "web"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ioulian/frontend-workflow",
  "repository": {
    "type": "git",
    "url": "git@github.com:ioulian/frontend-workflow.git"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "private": false,
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "dependencies": {
    "@fancyapps/fancybox": "3.5.7",
    "aria-tablist": "1.2.2",
    "bootstrap": "5.0.0-alpha3",
    "colcade": "0.2.0",
    "jquery": "3.5.1",
    "onfire.js": "2.0.1",
    "popper.js": "1.16.1",
    "smoothscroll-polyfill": "0.4.4",
    "swiper": "6.3.5",
    "throttle-debounce": "3.0.1",
    "vanilla-lazyload": "17.2.0",
    "workbox-cacheable-response": "5.1.4",
    "workbox-core": "5.1.4",
    "workbox-expiration": "5.1.4",
    "workbox-precaching": "5.1.4",
    "workbox-routing": "5.1.4",
    "workbox-strategies": "5.1.4"
  },
  "scripts": {
    "start": "NODE_ENV=devserver webpack serve",
    "start:d": "NODE_ENV=devserver webpack-dashboard -- webpack serve",
    "watch": "webpack --watch",
    "build": "yarn lint && yarn export",
    "export": "NODE_ENV=production webpack",
    "lint": "yarn tslint:lint && yarn eslint:lint && yarn stylelint:lint",
    "tslint:lint": "tsc --noEmit --project .tsconfig.json",
    "eslint:lint": "eslint 'src/**/*.{js,ts}'",
    "eslint:fix": "eslint --fix 'src/**/*.{js,ts}'",
    "stylelint:lint": "stylelint 'src/**/*.{scss,css}'",
    "stylelint:fix": "stylelint --fix 'src/**/*.{scss,css}'",
    "prettier:lint": "prettier --check 'src/**/*.{js,scss,css,ts}'",
    "prettier:fix": "prettier --write 'src/**/*.{js,scss,css,ts}'",
    "analyze": "NODE_ENV=production webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json --default-sizes gzip"
  },
  "devDependencies": {
    "@csstools/postcss-sass": "4.0.0",
    "@types/swiper": "5.4.0",
    "@types/throttle-debounce": "2.1.0",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "autoprefixer": "9.8.6",
    "clean-webpack-plugin": "3.0.0",
    "cosmiconfig": "7.0.0",
    "css-loader": "5.0.0",
    "cssnano": "4.1.10",
    "eslint": "7.11.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.14.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.1.4",
    "favicons-webpack-plugin": "5.0.0-alpha.1",
    "file-loader": "6.1.1",
    "html-loader": "1.3.2",
    "html-webpack-plugin": "4.5.0",
    "image-webpack-loader": "7.0.1",
    "mini-css-extract-plugin": "1.1.2",
    "node-sass": "4.14.1",
    "postcss-custom-properties": "10.0.0",
    "postcss-easing-gradients": "3.0.1",
    "postcss-loader": "4.0.4",
    "postcss-preset-env": "6.7.0",
    "postcss-utilities": "0.8.4",
    "prettier": "2.1.2",
    "prettier-webpack-plugin": "1.2.0",
    "sass-loader": "10.0.4",
    "script-loader": "0.7.2",
    "style-loader": "2.0.0",
    "stylelint": "13.7.2",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-prettier": "1.1.2",
    "stylelint-scss": "3.18.0",
    "svgo": "1.3.2",
    "svgo-loader": "2.2.1",
    "ts-loader": "8.0.6",
    "typescript": "4.0.3",
    "webpack": "5.2.0",
    "webpack-bundle-analyzer": "3.9.0",
    "webpack-cli": "4.1.0",
    "webpack-dashboard": "3.2.1",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.2.0",
    "webpack-stream": "6.1.0",
    "workbox-webpack-plugin": "5.1.4"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}
