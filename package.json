{
  "name": "frontend-workflow",
  "version": "1.0.0",
  "description": "Frontend workflow using Webpack 5",
  "author": "Yulian Alexeyev <yulian@dev-q.be> (https://github.com/ioulian)",
  "keywords": [
    "css",
    "js",
    "webpack",
    "typescript",
    "post-css",
    "prettier",
    "nvm",
    "stylelint",
    "eslint",
    "editorconfig",
    "favicons",
    "bootstrap",
    "es6",
    "drupal",
    "drupal8",
    "web",
    "storybook"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ioulian/frontend-workflow",
  "repository": {
    "type": "git",
    "url": "git@github.com:ioulian/frontend-workflow.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "private": false,
  "sideEffects": [
    "*.js",
    "*.css",
    "*.scss"
  ],
  "dependencies": {
    "@fancyapps/fancybox": "3.5.7",
    "@popperjs/core": "2.9.2",
    "aria-tablist": "1.2.2",
    "bootstrap": "5.0.0",
    "colcade": "0.2.0",
    "core-js": "3.12.0",
    "element-closest": "3.0.2",
    "element-matches-polyfill": "1.0.0",
    "intersection-observer": "0.12.0",
    "jquery": "3.6.0",
    "onfire.js": "2.0.1",
    "polytype": "0.13.2",
    "promise-polyfill": "8.2.0",
    "smoothscroll-polyfill": "0.4.4",
    "swiper": "6.5.9",
    "throttle-debounce": "3.0.1",
    "vanilla-lazyload": "17.3.1",
    "workbox-cacheable-response": "6.1.5",
    "workbox-core": "6.1.5",
    "workbox-expiration": "6.1.5",
    "workbox-precaching": "6.1.5",
    "workbox-routing": "6.1.5",
    "workbox-strategies": "6.1.5"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=devserver webpack serve",
    "start:d": "cross-env NODE_ENV=devserver webpack-dashboard -- webpack serve",
    "watch": "webpack --watch",
    "build": "yarn lint && yarn export",
    "build:all": "yarn lint && yarn export && yarn build-storybook",
    "export": "cross-env NODE_ENV=production webpack",
    "lint": "yarn tslint:lint && yarn eslint:lint && yarn stylelint:lint",
    "tslint:lint": "tsc --noEmit --project .tsconfig.json",
    "eslint:lint": "eslint 'src/**/*.{js,ts}'",
    "eslint:fix": "eslint --fix 'src/**/*.{js,ts}'",
    "stylelint:lint": "stylelint 'src/**/*.{scss,css}'",
    "stylelint:fix": "stylelint --fix 'src/**/*.{scss,css}'",
    "prettier:lint": "prettier --check 'src/**/*.{js,scss,css,ts}'",
    "prettier:fix": "prettier --write 'src/**/*.{js,scss,css,ts}'",
    "analyze": "cross-env NODE_ENV=production webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json --default-sizes gzip",
    "storybook": "cross-env NODE_ENV=devserver IS_STORYBOOK=true start-storybook -p 6006",
    "build-storybook": "cross-env NODE_ENV=production IS_STORYBOOK=true build-storybook -o dist/storybook",
    "generate": "ts-node scripts/generate/index.ts"
  },
  "devDependencies": {
    "@babel/core": "7.14.0",
    "@beyonk/google-fonts-webpack-plugin": "1.7.0",
    "@storybook/addon-a11y": "6.2.9",
    "@storybook/addon-actions": "6.2.9",
    "@storybook/addon-essentials": "6.2.9",
    "@storybook/addon-links": "6.2.9",
    "@storybook/addon-storysource": "6.2.9",
    "@storybook/html": "6.2.9",
    "@types/swiper": "5.4.2",
    "@types/throttle-debounce": "2.1.0",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "autoprefixer": "10.2.5",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "4.0.0-alpha.0",
    "cosmiconfig": "7.0.0",
    "cross-env": "7.0.3",
    "css-loader": "5.2.4",
    "cssnano": "5.0.2",
    "esbuild-loader": "2.13.0",
    "eslint": "7.25.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.4.0",
    "favicons": "6.2.1",
    "favicons-webpack-plugin": "5.0.2",
    "file-loader": "6.2.0",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.3.1",
    "image-webpack-loader": "7.0.1",
    "mini-css-extract-plugin": "1.6.0",
    "node-sass": "5.0.0",
    "param-case": "3.0.4",
    "pascal-case": "3.1.2",
    "postcss": "8.2.14",
    "postcss-custom-properties": "11.0.0",
    "postcss-easing-gradients": "3.0.1",
    "postcss-loader": "5.2.0",
    "postcss-preset-env": "6.7.0",
    "postcss-utilities": "0.8.4",
    "prettier": "2.2.1",
    "prettier-webpack-plugin": "1.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass-loader": "11.0.1",
    "script-loader": "0.7.2",
    "style-loader": "2.0.0",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-prettier": "1.2.0",
    "stylelint-scss": "3.19.0",
    "svg-sprite-loader": "6.0.6",
    "svgo": "2.3.0",
    "svgo-loader": "3.0.0",
    "ts-loader": "9.1.2",
    "ts-node": "9.1.1",
    "typescript": "4.2.4",
    "webpack": "5.36.2",
    "webpack-bundle-analyzer": "4.4.1",
    "webpack-clean-obsolete-chunks": "0.4.0",
    "webpack-cli": "4.7.0",
    "webpack-dashboard": "3.3.3",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.7.3",
    "webpack-stream": "6.1.2",
    "workbox-webpack-plugin": "6.1.5",
    "yargs": "17.0.1"
  },
  "browserslist": [
    "defaults"
  ]
}
